[*.cs]
insert_final_newline = true
indent_style = space
indent_size = 4

# StyleCop.Analyzers

# SA1101PrefixLocalCallsWithThis
dotnet_diagnostic.SA1101.severity = None

# SA1204StaticElementsMustAppearBeforeInstanceElements.
